<?xml version="1.0" encoding="UTF-8"?>
<MOML>
<FUNCTIONLIST>
<FUNCTION id="webBack">
<FUNCTIONITEM cmd="{ google_web.back}" />
</FUNCTION>
<FUNCTION id="webForward">
<FUNCTIONITEM cmd="{ google_web.forward }" />
</FUNCTION>
<FUNCTION id="webRefresh">
<FUNCTIONITEM cmd="{ google_web.refresh}" />
</FUNCTION>
</FUNCTIONLIST>
<UI>
<UILAYOUT portrait="480,800">
<WINDOW layout="0,0,480,800" defaultImg="#ffffff" >
<WEBVIEW id="google_web" layout="0,0,480,700" loadSrc="http://nikemania.com" clientControl="true" />
<WINDOW layout="0,700,480,100" align="linear:horizontal">
<BUTTON layout="160,100" text="BACK" onClick="{function.webBack}"/>
<BUTTON layout="160,100" text="FORWARD" onClick="{function.webForward}"/>
<BUTTON layout="160,100" text="REFRESH" onClick="{function.webRefresh}"/>
</WINDOW>
</WINDOW>
</UILAYOUT>
</UI>
</MOML>